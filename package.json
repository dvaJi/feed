{
  "name": "@dvaji/feed",
  "version": "2.1.0",
  "description": "Feed is a RSS, Atom and JSON feed generator for Node.js, making content syndication simple and intuitive!",
  "homepage": "https://github.com/dvaJi/feed",
  "author": "Jean-Philippe Monette <contact@jpmonette.net>",
  "license": "MIT",
  "main": "lib/feed.js",
  "types": "lib/feed.d.ts",
  "scripts": {
    "build": "rm -rf lib/ && mkdir lib && tsc",
    "prepublish": "npm run build",
    "test": "cross-env NODE_ENV=test && jest --silent",
    "test-travis": "cross-env NODE_ENV=test && jest --coverage"
  },
  "keywords": [
    "rss",
    "atom",
    "feed",
    "syndication",
    "xml",
    "wrapper",
    "blog"
  ],
  "dependencies": {
    "luxon": "^1.11.3",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/xml": "^1.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "typescript": "^3.0.1"
  },
  "engines": {
    "node": ">=0.4.0"
  },
  "bugs": {
    "url": "https://github.com/dvaJi/feed/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvaJi/feed.git"
  }
}
